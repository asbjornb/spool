{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ResponseEntry",
  "description": "Agent response entry",
  "type": "object",
  "required": [
    "content",
    "id",
    "ts"
  ],
  "properties": {
    "content": {
      "type": "string"
    },
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "model": {
      "type": [
        "string",
        "null"
      ]
    },
    "original_bytes": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint",
      "minimum": 0.0
    },
    "subagent_id": {
      "type": [
        "string",
        "null"
      ],
      "format": "uuid"
    },
    "token_usage": {
      "anyOf": [
        {
          "$ref": "#/definitions/TokenUsage"
        },
        {
          "type": "null"
        }
      ]
    },
    "truncated": {
      "type": [
        "boolean",
        "null"
      ]
    },
    "ts": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "additionalProperties": true,
  "definitions": {
    "TokenUsage": {
      "description": "Token usage information for an API response",
      "type": "object",
      "required": [
        "input_tokens",
        "output_tokens"
      ],
      "properties": {
        "cache_creation_tokens": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "cache_read_tokens": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "input_tokens": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "output_tokens": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    }
  }
}