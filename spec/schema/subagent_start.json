{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SubagentStartEntry",
  "description": "Subagent start entry",
  "type": "object",
  "required": [
    "agent",
    "id",
    "ts"
  ],
  "properties": {
    "agent": {
      "type": "string"
    },
    "context": {
      "type": [
        "string",
        "null"
      ]
    },
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "parent_subagent_id": {
      "type": [
        "string",
        "null"
      ],
      "format": "uuid"
    },
    "ts": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "additionalProperties": true
}