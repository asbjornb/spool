{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ThinkingEntry",
  "description": "Agent thinking entry",
  "type": "object",
  "required": [
    "content",
    "id",
    "ts"
  ],
  "properties": {
    "collapsed": {
      "type": [
        "boolean",
        "null"
      ]
    },
    "content": {
      "type": "string"
    },
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "original_bytes": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint",
      "minimum": 0.0
    },
    "subagent_id": {
      "type": [
        "string",
        "null"
      ],
      "format": "uuid"
    },
    "truncated": {
      "type": [
        "boolean",
        "null"
      ]
    },
    "ts": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  },
  "additionalProperties": true
}